<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <!-- Repos -->
    <preference for="Praxigento\Accounting\Repo\Entity\IAccount"
                type="Praxigento\Accounting\Repo\Entity\Def\Account"/>
    <preference for="Praxigento\Accounting\Repo\Entity\IOperation"
                type="Praxigento\Accounting\Repo\Entity\Def\Operation"/>
    <preference for="Praxigento\Accounting\Repo\Entity\ITransaction"
                type="Praxigento\Accounting\Repo\Entity\Def\Transaction"/>
    <preference for="Praxigento\Accounting\Repo\Entity\Type\IAsset"
                type="Praxigento\Accounting\Repo\Entity\Type\Def\Asset"/>
    <preference for="Praxigento\Accounting\Repo\Entity\Type\IOperation"
                type="Praxigento\Accounting\Repo\Entity\Type\Def\Operation"/>
    <preference for="Praxigento\Accounting\Lib\Repo\IModule"
                type="Praxigento\Accounting\Lib\Repo\Def\Module"/>

    <!-- Services -->
    <preference for="Praxigento\Accounting\Lib\Service\IAccount"
                type="Praxigento\Accounting\Lib\Service\Account\Call"/>
    <preference for="Praxigento\Accounting\Lib\Service\IBalance"
                type="Praxigento\Accounting\Lib\Service\Balance\Call"/>
    <preference for="Praxigento\Accounting\Lib\Service\IOperation"
                type="Praxigento\Accounting\Lib\Service\Operation\Call"/>
    <preference for="Praxigento\Accounting\Lib\Service\ITransaction"
                type="Praxigento\Accounting\Lib\Service\Transaction\Call"/>

    <!-- API Services -->
    <preference for="Praxigento\Accounting\Api\AccountInterface"
                type="Praxigento\Accounting\Api\Def\Account"/>

</config>
